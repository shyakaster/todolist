<div class="container">
  <div class="center-block">
    <h2> Todos</h2>
  </div>
  <div class="col-md-8 well">
    <%@listings.each do |listing|%>
        <ul>
          <li><%= link_to listing.description,listing_path(listing) %></li>
          <li><%= link_to "Edit listing",edit_listing_path(listing) %></li>
          <li><%= link_to "Destroy",listing, method: :delete, data: { confirm: "Are you sure ?" } %></li>
        </ul>
    <%end%>
  </div>
  <div class="col-md-8">
    <button class="btn btn-default"><%= link_to "Create listing", new_listing_path %></button>
  </div>

</div>


